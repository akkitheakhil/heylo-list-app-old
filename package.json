{
  "name": "heylo-list",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve::dev",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve::app": "ng serve --proxy-config proxy-config.json --host 0.0.0.0 --disable-host-check --poll",
    "serve::dev": "concurrently \"npm run serve::app\" \"npm run serve::mock-server\"",
    "serve::mock-server": "cd mock_server && nodemon server.ts"
  },
  "private": true,
